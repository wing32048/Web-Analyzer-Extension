{
    "manifest_version": 3,
    "name": "WEB ANALYZER",
    "version": "1.0",
    "permissions": [
        "tabGroups",
        "notifications",
        "cookies",
        "storage"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["js/main.js"],
            "run_at": "document_start"
        }
    ],
    "action":{
        "default_popup":"./popup/html/setting.html"
    },
    "background" : {
        "service_worker" : "js/background.js"
    }
}