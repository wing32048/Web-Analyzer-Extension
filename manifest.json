{
    "manifest_version": 3,
    "name": "WEB ANALYZER",
    "version": "1.0",
    "permissions": [
        "tabs",
        "notifications",
        "cookies",
        "storage"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "exclude_matches": ["https://192.168.140.100/account.php"],
            "js": ["js/main.js"],
            "run_at": "document_start"
        },{
            "matches": ["https://192.168.140.100/account.php"],
            "js": ["js/account.js"],
            "run_at": "document_start"
        }
    ],
    "action": {
        "default_popup": "popup/html/index.html"
    },
    "background": {
        "service_worker": "js/background.js",
        "type": "module"
    }
}