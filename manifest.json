{
  "manifest_version": 3,
  "name": "WEB ANALYZER",
  "version": "1.0",
  "permissions": [
    "tabGroups",
    "http://127.0.0.1/connect.php",
    "notifications"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/main.js"],
      "run_at": "document_start"
    }
  ],
  "action":{
    "default_popup":"./popup/html/signin/signin.html"
  },
  "background" : {
    "service_worker" : "js/background.js"
  }
}