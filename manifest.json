{
    "manifest_version": 3,
    "name": "fyp_with_virustotal",
    "version": "1.0.0",
    "content_scripts": [
    {
        "matches":["<all_urls>"],
        "exclude_matches": ["*://www.google.com/*",
        "*://search.yahoo.com/*",
        "*://www.bing.com/"],
        "js":["script.js"],
        "persistent": false
    }],
    "permissions": ["storage"]
}