{
  "manifest_version": 3,
  "name": "Your Extension",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "@content_security_policy": {
    "extension_pages":"script-src 'self' 'unsafe-eval'; object-src 'self'"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["main.js"],
      "run_at": "document_start"
    }
  ]
}